{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM"],
    "moduleResolution": "node",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "noEmit": true,
    "allowJs": true
  },
  "include": [
    "forgebot*.ts",
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "build",
    "kypria_automation_orchestrator (1).tsx",
    "kypria_worker_deno.ts"
  ]
}
